
@{
    ViewBag.Title = "EmployeeRegistration";
    Layout = "~/Views/Shared/EsselMaster.cshtml";
}

@*<h2>EmployeeRegistration</h2>*@
@Styles.Render("~/bundles/Accounts/css")
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <div class="col-md-12">
                        <div class="col-md-6">
                            <h4 style="text-align:left;font-weight:bold">Employee Registration</h4>
                        </div>
                        <div class="col-md-6" style="text-align:right">
                         
                        </div>
                    </div>
                   
                </div>
                <div class="body">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12">
                            <!-- Example Tabs Left-->
                            <div class="card-inner">
                                <div class="col-md-12">
                                    <div class="row" id="divNewEmp"><br/>
                                        @Html.Action("StaffRegistration", "HR")
                                        @*new emp*@
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
 </div>
<script type="text/javascript">
    $(document).ready(function () {
      
        $("#divNewEmp").removeClass("hidden");
    });
    function ViewEmpDetails() {    
        $("#divNewEmp").addClass("hidden");
    }
</script>

